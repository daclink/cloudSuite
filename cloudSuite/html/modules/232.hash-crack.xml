        <version>2.3</version>
    </systemRequirement>
    <fieldset>
        <legend>Hash File</legend>
        <element id="234">
            <type>text</type>
            <name>infile</name>
            <value>-i</value>
            <description>The file to crack</description>
            <input>text file</input>
            <output>text file</output>
            <required>1</required>
            <default></default>
            <selected>false</selected>
        </element>
    </fieldset>
    <fieldset>
