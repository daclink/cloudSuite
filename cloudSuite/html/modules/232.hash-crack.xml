            <output></output>
            <required>0</required>
            <default></default>
            <selected>false</selected>
        </element>
        <element id="236">
            <type>checkbox</type>
            <name>dictionaries[]</name>
            <value>--russian</value>
            <description>Russian Language</description>
            <input></input>
            <output></output>
            <required>0</required>
            <default></default>
            <selected>true</selected>
        </element>
        <element id="237">
            <type>checkbox</type>
            <name>dictionaries[]</name>
            <value>--french</value>
            <description>French Language</description>
            <input></input>
            <output></output>
            <required>0</required>
            <default></default>
            <selected>true</selected>
